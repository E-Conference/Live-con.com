 <div id="resultsFound"><h4>{{ nbResult }} results found</h4></div>
 <table class="table table-hover records_list">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Color</th>
                    <th>Name</th>
                    <th>Label</th>
                    <th colspan="3">{% trans %}Actions{% endtrans %}</th>
                </tr>
            </thead>
            <tbody>
            {% for entity in pager.currentPageResults %}
                <tr>
                    <td><a href="{{ path('schedule_category_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>
                      <div style="width:28px;height:28px;border:1px solid #222;background:{{ entity.color }}" title="{{ entity.color }}">
                      </div>
                    </td>                    <td>{{ entity.name }}</td>
                    <td>{{ entity.label }}</td>
                    <td>
                        <a class="btn btn-block btn-default btn-small" href="{{ path('schedule_category_show', { 'id': entity.id }) }}"><i class="fa fa-eye"></i></a>
                    </td>
                    <td>
                        <a class="btn btn-block btn-warning btn-small" href="{{ path('schedule_category_edit', { 'id': entity.id }) }}"><i class="fa fa-pencil"></i></a>
                     </td> 
                </tr>
            {% endfor %}
            </tbody>
        </table>

        <p>
            {{ pagerfanta(pager, 'twitter_bootstrap') }}
        </p>