{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}



<div class="page-header">
  <h1>{% trans %}Category creation{% endtrans %}</h1>
</div>

<form action="{{ path('schedule_category_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-primary"> <span class="icon-ok	glyphicon-white"></span></button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a class="btn" href="{{ path('schedule_category') }}">
            <span class="icon-list glyphicon-white"></span>
        </a>
    </li>
</ul>
{% endblock body %}
