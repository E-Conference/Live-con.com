{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}
{{ parent() }}



<div class="page-header">
  <h1>{% trans %}Category creation{% endtrans %}</h1>
</div>

<form action="{{ path('schedule_category_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <div class="row-fluid">
            <div class="col-lg-9"></div>
            <div class="col-lg-2">
                  <button type="submit" class="btn btn-default"><i class="icon-ok"></i></button>
                  <a class="btn btn-warning" href="{{ path('schedule_category') }}">
                    <i class="icon-th-list"></i>
                </a>
            </div>
        </div>
</form>

{% endblock body %}
