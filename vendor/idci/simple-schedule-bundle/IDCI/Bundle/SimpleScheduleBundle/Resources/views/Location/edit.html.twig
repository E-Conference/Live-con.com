{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}
{{ parent() }}



<div class="page-header">
  <h1>{% trans %}Edit{% endtrans %}</h1>
</div>

<form action="{{ path('schedule_location_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}

     <div class="row-fluid">
            <div class="col-lg-9"></div>
            <div class="col-lg-2">
                  <button type="submit" class="btn btn-default"><i class="icon-ok"></i></button>
                  <a class="btn btn-warning" href="{{ path('schedule_location') }}">
                    <i class="icon-th-list"></i>
                </a>
                <form action="{{ path('schedule_location_delete', { 'id': entity.id }) }}" method="post">
                  {{ form_widget(delete_form) }}
                  <button type="submit" class="btn btn-danger"><i class="icon-remove"></i></button>
                </form>
            </div>
        </div>
</form>



 <div id="equipment_manage"> 
      {% include "IDCISimpleScheduleBundle:Location:equipmentsRelation.html.twig" %}
</div>      

{% endblock body %}
