{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}



<div class="page-header">
  <h1>{% trans %}Edit{% endtrans %}</h1>
</div>

<form action="{{ path('schedule_location_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit" class="btn btn-default"><span class="icon-ok"></span></button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a class="btn btn-success" href="{{ path('schedule_location') }}">
            <span class="icon-list  glyphicon-white"></span>
        </a>
    </li>
    <li>
        <form action="{{ path('schedule_location_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" class="btn btn-danger"><span class="icon-remove  glyphicon-white"></span></button>
        </form>
    </li>
</ul>
{% endblock body %}
