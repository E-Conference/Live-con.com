{% extends "IDCISimpleScheduleBundle::adminbase.html.twig" %}

{% block body %}

{{ parent() }}

<div class="page-header">
  <h1>{% trans %}Status creation{% endtrans %}</h1>
</div>

<form action="{{ path('admin_schedule_status_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p></br>
        <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-ok"></span></button>
		<a class="btn btn-warning" href="{{ path('admin_schedule_status') }}">
            <span class="glyphicon glyphicon-list glyphicon-white"></span>
        </a>
    </p>
</form>
{% endblock body %}
