{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}

{{ parent() }}


<div class="page-header">
  <h1>{% trans %}Edit{% endtrans %}</h1>
</div>

<form action="{{ path('schedule_event_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(form) }}>
    {% include "IDCISimpleScheduleBundle:Event:form.html.twig" %}
     <div class="row-fluid">
            <div class="col-lg-9"></div>
            <div class="col-lg-2">
                  <button type="submit" class="btn btn-default"><i class="icon-ok"></i></button>
                  <a class="btn btn-warning" href="{{ path('admin_schedule_event') }}">
                    <i class="icon-th-list"></i>
                </a>
            </div>
        </div>
</form>
 <form action="{{ path('admin_schedule_event_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" class="btn btn-danger"><i class="icon-remove"></i></button>
        </form>
{% include "IDCISimpleScheduleBundle:Event:xproperties.html.twig" with {"with_form": true} %}


