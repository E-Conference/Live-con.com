{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}

{{ parent() }}


<div class="page-header">
  <h1>{% trans %}Status Edit{% endtrans %}</h1>
</div>
<div class="row">
  <div class="col-lg-2"></div>
  <div class="col-lg-8">
    <form action="{{ path('schedule_status_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
        {{ form_widget(edit_form) }}
   </div>
   <div class="row-fluid">
        <div class="col-lg-5"></div>
        <div class="col-lg-2">
            </br><button type="submit" class="btn btn-default"><i class="icon-ok"></i></button>    
          </div>
    </form>
    </div>

    <div class="row">
        <div class="col-lg-2"></div>
          <div class="btn-group">
            <form action="{{ path('schedule_status_delete', { 'id': entity.id }) }}" method="post">
                <input type="hidden" name="_method" value="DELETE" />
                {{ form_widget(delete_form) }}
                 <button type="submit" class="btn btn-danger"><i class="icon-remove"></i></button>
                  <a type="button" class="btn btn-warning " href="{{ path('schedule_status') }}">
                    <i class="icon-th-list"></i>
                </a>
            </form>
          </div>
    </div>
   
{% endblock body %}
