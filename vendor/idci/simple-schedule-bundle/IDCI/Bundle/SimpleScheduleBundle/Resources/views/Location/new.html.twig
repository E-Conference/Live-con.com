{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block title %}New location{% endblock title %}

{% block centerPanel %}
  {{ parent() }}
      <h1>{% trans %}Location creation{% endtrans %}</h1>

  <div class="row">
     <div class="col-lg-5"></div>
          <div class="col-lg-2 btn-group">
              <a type="button" href="{{ path('schedule_event') }}" class="btn btn-warning">
                    <i class="fa fa-list"></i>
              </a>
          </div>    
  </div> 


    <form action="{{ path('schedule_location_create') }}" method="post" {{ form_enctype(form) }}>
      <div class="row">
           <div class="col-lg-2"></div>
           <div class="col-lg-4">
        {{ form_widget(form) }}
        </br>
        <button type="submit" class="btn btn-block btn-success">
                        <i class="fa fa-check"></i>
              </button>
        </div>
        
    </form>
  </div>
</div>
{% endblock %}
