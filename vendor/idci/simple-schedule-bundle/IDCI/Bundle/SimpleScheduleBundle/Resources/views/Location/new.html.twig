{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}

{{ parent() }}


<div class="page-header">
  <h1>{% trans %}Location creation{% endtrans %}</h1>
</div>
<div class="row">
  <div class="col-lg-2"></div>
  <div class="col-lg-8">
      <form action="{{ path('schedule_location_create') }}" method="post" {{ form_enctype(form) }}>
          {{ form_widget(form) }}
          <div class="row-fluid">
                  <div class="col-lg-5"></div>
                  <div class="col-lg-2">
                        <button type="submit" class="btn btn-default"><i class="icon-ok"></i></button>
                        <a class="btn btn-warning" href="{{ path('schedule_location') }}">
                          <i class="icon-th-list"></i>
                      </a>
                  </div>
              </div>
      </form>
    </div>
  </div>
{% endblock body %}
