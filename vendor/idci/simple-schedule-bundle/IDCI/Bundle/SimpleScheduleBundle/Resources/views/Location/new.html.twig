{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block title %}New location{% endblock title %}

{% block centerPanel %}
  {{ parent() }}
       <div class="row page-top-header ">
            <div class="page-title col-xs-5  col-sm-4 col-md-3 col-lg-2">
                <span>New location</span>
            </div>

            <div class="col-xs-2 col-sm-4 col-md-6 col-lg-8">
            </div>
            <div class="page-btn-list col-xs-5  col-sm-4 col-md-3 col-lg-2">
                 <a type="button" href="{{ path('schedule_location') }}" class="btn btn-warning" title="Go back to the list">
                  <i class="fa fa-list"></i>
                 </a>
            </div>
      </div>
      <div class="row page-content">

      <form action="{{ path('schedule_location_create') }}" method="post" {{ form_enctype(form) }}>
        <div class="row">
             <div class="col-lg-2"></div>
             <div class="col-lg-4">
          {{ form_widget(form) }}
          </br>
          <button type="submit" class="btn btn-block btn-success">
                          <i class="fa fa-check"></i>
                </button>
          </div>
          
      </form>
  </div>

{% endblock %}
