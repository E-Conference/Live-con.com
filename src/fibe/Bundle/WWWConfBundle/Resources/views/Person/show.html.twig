{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body -%}
    <h1>Person</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Flag_schedule</th>
                <td>{{ entity.flagschedule }}</td>
            </tr>
            <tr>
                <th>Flag_schedule_admin</th>
                <td>{{ entity.flagscheduleadmin }}</td>
            </tr>
            <tr>
                <th>Flag_data</th>
                <td>{{ entity.flagdata }}</td>
            </tr>
            <tr>
                <th>Flag_data_admin</th>
                <td>{{ entity.flagdataadmin }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Agent</th>
                <td>{{ entity.agent }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Img</th>
                <td>{{ entity.img }}</td>
            </tr>
            <tr>
                <th>Depiction</th>
                <td>{{ entity.depiction }}</td>
            </tr>
            <tr>
                <th>Familyname</th>
                <td>{{ entity.familyName }}</td>
            </tr>
            <tr>
                <th>Givenname</th>
                <td>{{ entity.givenName }}</td>
            </tr>
            <tr>
                <th>Based_near</th>
                <td>{{ entity.basednear }}</td>
            </tr>
            <tr>
                <th>Knows</th>
                <td>{{ entity.knows }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ entity.age }}</td>
            </tr>
            <tr>
                <th>Made</th>
                <td>{{ entity.made }}</td>
            </tr>
            <tr>
                <th>Primary_topic</th>
                <td>{{ entity.primarytopic }}</td>
            </tr>
            <tr>
                <th>Project</th>
                <td>{{ entity.project }}</td>
            </tr>
            <tr>
                <th>Organization</th>
                <td>{{ entity.organization }}</td>
            </tr>
            <tr>
                <th>_group</th>
                <td>{{ entity.group }}</td>
            </tr>
            <tr>
                <th>Member</th>
                <td>{{ entity.member }}</td>
            </tr>
            <tr>
                <th>Document</th>
                <td>{{ entity.document }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ entity.image }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('person_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('person_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('person_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
