{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block title %}Role{% endblock title %}

{% block centerPanel %}
    {{ parent() }}


    <div class="row page-top-header ">
        <div class="page-title  col-xs-5  col-sm-4 col-md-3 col-lg-2">
            <span>Role list</span>
            <a class="btn btn-info" href="{{ path('documentation', { 'anchor': 'Role' }) }}" target="_blank" title="Help">
                <i class="fa fa-question"></i>
            </a>
        </div>

        <div class=" col-xs-2 col-sm-4 col-md-6 col-lg-8">
        </div>

        <div class="page-btn-list  col-xs-5  col-sm-4 col-md-3 col-lg-2">
            <a class="btn btn-info btn-create create" href="{{ path('schedule_roletype_new') }}" title="Add a role">
                <i class="fa fa-plus"></i> New role
            </a>
        </div>
    </div>

    <div class="row page-content">
        <div id="person_list" class="table-responsive">
            {% include "fibeWWWConfBundle:RoleType:list.html.twig" %}
        </div>
    </div>

{% endblock %}