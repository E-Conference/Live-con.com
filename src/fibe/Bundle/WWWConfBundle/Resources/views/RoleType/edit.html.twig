{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}
{{ parent() }}
<div class="container">
    <div class="row center">
    <h1>RoleType edit</h1>

    <form action="{{ path('schedule_roletype_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
        <input type="hidden" name="_method" value="PUT" />
        {{ form_widget(edit_form) }}
        <p>
            <button class="btn btn-primary" type="submit"> <i class="icon-edit"></i> Save</button>
        </p>
    </form>
    <a class="btn btn-warning" href="{{ path('schedule_roletype') }}">
        <i class="icon-th-list"></i> List
    </a>
    <form action="{{ path('schedule_roletype_delete', { 'id': entity.id }) }}" method="post">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit">
            <i class="icon-trash"></i> Delete
        </button>
    </form> 
    </div>
</div>
{% endblock %}
