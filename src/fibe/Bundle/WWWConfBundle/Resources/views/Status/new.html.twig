{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}

  {{ parent() }}


  <div class="page-header">
    <h1>{% trans %}Status creation{% endtrans %}</h1>
  </div>

  <form action="{{ path('schedule_status_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p><br/>
      <button type="submit" class="btn btn-default"><i class="fa fa-check"></i></button>
      <a class="btn btn-warning" href="{{ path('schedule_status') }}">
        <i class="icon-list"></i>
      </a>
    </p>
  </form>
{% endblock body %}
