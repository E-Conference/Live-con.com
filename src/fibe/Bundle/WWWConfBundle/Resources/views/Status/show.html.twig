{% extends 'fibeWWWConfBundle::base.html.twig' %}

{% block body %}

  {{ parent() }}


  <div class="page-header">
    <h1>Status</h1>
  </div>

  <table class="table table-hover record_properties">
    <tbody>
    <tr>
      <th>Id</th>
      <td>{{ entity.id }}</td>
    </tr>
    <tr>
      <th>Value</th>
      <td>{{ entity.value }}</td>
    </tr>
    </tbody>
  </table>


  <a class="btn btn-success" href="{{ path('schedule_status') }}">
    <i class="icon-list"></i>
  </a>

  <a class="btn btn-warning" href="{{ path('schedule_status_edit', { 'id': entity.id }) }}">
    <i class="fa fa-pencil-square-o"></i>
  </a>

{% endblock body %}
