    <!-- http://xmlns.com/foaf/0.1/Person -->

    <Class rdf:about="&foaf;Person"/>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    

    <NamedIndividual rdf:about="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}">
        <rdf:type rdf:resource="&swc;ConferenceEvent"/>
        <rdfs:label>{{ entity.getConfName() }}</rdfs:label>
        <ical:dtstart rdf:datatype="&xsd;date">{{ entity.getMainConfEvent().getStartAt()| date('Y-m-d H:i')}}</ical:dtstart>
        <ical:dtend rdf:datatype="&xsd;date">{{ entity.getMainConfEvent().getEndAt()| date('Y-m-d H:i')}}</ical:dtend>
        <swc:hasAcronym>{{ entity.getMainConfEvent().getAcronym()}}</swc:hasAcronym>
        <foaf:homepage rdf:resource="{{ entity.getMainConfEvent().getUrl() }}"/>
        {% for role in entity.mainConfEvent.roles %}
        <swc:hasRole rdf:resource="http://data.live-con.com/page/role/{{entity.mainConfEvent.id}}/{{role.type.name}}"/>
        {% endfor %}
        <swc:hasLogo rdf:resource="{{ entity.getLogoPath() }}"/>
        <swc:hasLocation rdf:resource="http://data.live-con.com/page/location/{{entity.getMainConfEvent().getLocation().getId()}}/{{entity.getMainConfEvent().getLocation().getSlug()}}"/>
        <swc:hasRelatedDocument rdf:resource="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}/proceedings"/>
        {% for event in entity.getEvents() %}
            {% if event.isMainConfEvent == false %}
                 <swc:isSuperEventOf rdf:resource="http://data.live-con.com/page/event/{{ event.id }}/{{ event.slug }}"/>
            {% endif %}
        {% endfor %}
    </NamedIndividual>

 

    <NamedIndividual rdf:about="http://data.live-con.com/page/role/{{entity.mainConfEvent.id}}/Delegate">
        <rdf:type rdf:resource="&swc;Delegate"/>
        <rdfs:label>{{ entity.getConfName() }}  delegate</rdfs:label>
        <swc:isRoleAt rdf:resource="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}"/>
        {% for role in entity.mainConfEvent.getRoles() %}
            {% if role.type =="Delegate" %}
                <swc:heldBy rdf:resource="http://data.live-con.com/page/person/{{role.person.id}}/{{role.person.slug}}"/>
            {% endif %}
        {% endfor %}
    </NamedIndividual>

    <NamedIndividual rdf:about="http://data.live-con.com/page/role/{{entity.mainConfEvent.id}}/Chair">
        <rdf:type rdf:resource="&swc;Chair"/>
        <rdfs:label>{{ entity.getConfName() }}  chair</rdfs:label>
        <swc:isRoleAt rdf:resource="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}"/>
        {% for role in entity.mainConfEvent.getRoles() %}
            {% if role.type =="Chair" %}
                <swc:heldBy rdf:resource="http://data.live-con.com/page/person/{{role.person.id}}/{{role.person.slug}}"/>
            {% endif %}
        {% endfor %}
    </NamedIndividual>

    <NamedIndividual rdf:about="http://data.live-con.com/page/role/{{entity.mainConfEvent.id}}/Presenter">
        <rdf:type rdf:resource="&swc;Presenter"/>
        <rdfs:label>{{ entity.getConfName() }}  presenter</rdfs:label>
        <swc:isRoleAt rdf:resource="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}"/>
        {% for role in entity.mainConfEvent.getRoles() %}
            {% if role.type =="Presenter" %}
                <swc:heldBy rdf:resource="http://data.live-con.com/page/person/{{role.person.id}}/{{role.person.slug}}"/>
            {% endif %}
        {% endfor %}
    </NamedIndividual>

    <NamedIndividual rdf:about="http://data.live-con.com/page/role/{{entity.mainConfEvent.id}}/ProgrammeCommitteeMember">
        <rdf:type rdf:resource="&swc;ProgrammeCommitteeMember"/>
        <rdfs:label>{{ entity.getConfName() }} programme committee member</rdfs:label>
        <swc:isRoleAt rdf:resource="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}"/>
        {% for role in entity.mainConfEvent.getRoles() %}
            {% if role.type =="ProgrammeCommitteeMember" %}
                <swc:heldBy rdf:resource="http://data.live-con.com/page/person/{{role.person.id}}/{{role.person.slug}}"/>
            {% endif %}
        {% endfor %}
    </NamedIndividual>



    <NamedIndividual rdf:about="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}/proceedings">
        <rdf:type rdf:resource="&swrc;Proceedings"/>
        <rdfs:label>Proceedings of {{ entity.getConfName() }}</rdfs:label>
        <swrc:booktitle>Proceedings of {{ entity.getConfName() }}</swrc:booktitle>
        <swc:relatedToEvent rdf:resource="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}"/>
        {% for paper in entity.papers %}
        <swc:hasPart rdf:resource="http://data.live-con.com/page/paper/{{ paper.id }}/{{ paper.slug }}"/>
        {% endfor %}
    </NamedIndividual>


  {% for event in entity.events %}
  
     {% if event.isMainConfEvent == false %}
        <NamedIndividual rdf:about="http://data.live-con.com/page/event/{{event.id}}/{{event.slug}}">
         {% for category in event.categories %}
            <rdf:type rdf:resource="&swc;{{category.name}}"/>
         {% endfor %}
            <rdfs:label>{{event.summary}}</rdfs:label>
            <dc:title>{{event.summary}}</dc:title>
            <ical:dtstart>{{entity.startAt | date('Y-m-d H:i')}}</ical:dtstart>
            <ical:dtend>{{entity.endAt | date('Y-m-d H:i')}}</ical:dtend>
            <ical:duration>{{entity.duration}}</ical:duration>
            <ical:description>{{entity.description}}</ical:description>
            <swrc:abstract>{{entity.description}}</swrc:abstract>
            {% for paper in event.papers %}
            <swc:hasRelatedDocument rdf:resource="http://data.live-con.com/page/paper/{{paper.id}}/{{paper.slug}}"/>
            {% endfor %}
            <swc:isSubEventOf rdf:resource="http://data.live-con.com/page/event/{{event.parent.id}}/{{event.parent.slug}}"/>
            {% for role in event.roles %}
            <swc:hasRole rdf:resource="http://data.live-con.com/page/role/{{role.event.id}}/{{role.person.id}}/{{role.type.name}}"/>
            {% endfor %}
            {% if entity.location %}
            <swc:hasLocation rdf:resource="http://data.live-con.com/page/location/{{entity.location.id}}/{{entity.location.getSlug()}}"/>
            {% endif %}
            {% for topic in event.topics %}
            <dc:subject>{{topic.name}}</dc:subject>
            {% endfor %}
        </NamedIndividual>
      {% endif %}
  {% endfor %}



 {% for paper in entity.papers %}

    <NamedIndividual rdf:about="http://data.live-con.com/page/paper/{{paper.id}}/{{paper.slug}}">
        <rdf:type rdf:resource="&swrc;InProceedings"/>
        <rdfs:label>{{paper.title}}</rdfs:label>
        <swrc:date>{{entity.publishDate()}}</swrc:date>
        {% for topic in paper.topics %}
        <dc:subject>{{topic.name}}</dc:subject>
        {% endfor %}
        <swrc:abstract>{{paper.abstract}}</swrc:abstract>
        <dc:title>{{paper.title}}</dc:title>
        <swc:isPartOf rdf:resource="http://data.live-con.com/page/conference/{{ entity.id }}/{{ entity.slug }}/proceedings"/>
        {% for event in paper.events %}
        <swc:relatedToEvent rdf:resource="http://data.live-con.com/page/event/{{event.getParent().getId()}}/{{event.getParent().getSlug()}}"/>
        {% endfor %}
        {% for author in paper.authors %}
        <dc:creator rdf:resource="http://data.live-con.com/page/person/{{author.id}}/{{author.slug}}"/>
        <foaf:maker rdf:resource="http://data.live-con.com/page/person/{{author.id}}/{{author.slug}}"/>
        {% endfor %}
    </NamedIndividual>
 {% endfor %}


{% for organization in entity.organizations %}

    <NamedIndividual rdf:about="http://data.live-con.com/page/organization/{{organization.id}}/{{organization.slug}}">
        <rdf:type rdf:resource="&foaf;Organization"/>
        <rdfs:label>{{organization.name}}</rdfs:label>
        <foaf:name>{{organization.name}}</foaf:name>
        {% for person  in entity.members %}
        <foaf:member rdf:resource="http://data.live-con.com/page/person/{{person.id}}/{{person.slug}}"/>
        {% endfor %}
    </NamedIndividual>
{% endfor %}


{% for person in entity.persons %}

    <NamedIndividual rdf:about="http://data.live-con.com/page/person/{{person.id}}/{{person.slug}}">
        <rdf:type rdf:resource="&foaf;Person"/>
        <rdfs:label>{{person.firstName }} {{person.familyName}}</rdfs:label>
        <foaf:mbox_sha1sum>{{person.emailSha1}}</foaf:mbox_sha1sum>
        <foaf:lastName>{{person.familyName}}</foaf:lastName>
        <foaf:firstName>{{person.firstName}}</foaf:firstName>
        <foaf:name>{{person.firstName }} {{ person.familyName}}</foaf:name>
        {% for role in person.roles %}
        <swc:holdsRole rdf:resource="http://data.semanticweb.org/conference/{{ entity.id }}/{{ entity.slug }}/{{role.type.label}}"/>
        {% endfor %}
        {% for paper in person.papers %}
        <foaf:made rdf:resource="http://data.live-con.com/page/paper/{{paper.id}}/{{paper.slug}}"/>
        {% endfor %}
        {% for organization in person.organizations %}
        <swrc:affiliation rdf:resource="http://data.live-con.com/page/organization/{{organization.id}}/{{organization.slug}}"/>
        <foaf:based_near rdf:resource="http://dbpedia.org/resource/{{organization.country}}"/>
        {% endfor %}
    </NamedIndividual>
{% endfor %}

</rdf:RDF>



