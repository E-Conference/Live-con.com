
{% block body %}



<div class="container">
    <div class="row center">
        <table class=" table table-hover">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Page</th>
                    <th>Country</th>
                </tr>
            </thead>
            <tbody>
            {% for entity in entities %}
                <tr>
                    <td>{{ entity.name }}</td>
                    <td>{{ entity.page }}</td>
                    <td>{{ entity.country }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
{% endblock %}
