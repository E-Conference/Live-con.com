{% extends 'fibeConferenceBundle::base.html.twig' %}

{% block title %}Conference Settings{% endblock title %}

{% block centerPanel %}
    {{ parent() }}

  <div class="row page-top-header ">
            <div class="page-title col-xs-5  col-sm-4 col-md-3 col-lg-2">
                <span>Modules managment</span>
            </div>

      </div>
      <div class="row page-content">
     
   
          <form action="{{ path('schedule_module_update', { 'id': module.id }) }}" method="post" class="form-inline" role="form" {{ form_enctype(module_form) }}>
              <input type="hidden" name="_method" value="PUT" />
                  <div class="row">
                     <div class="col-lg-2"></div>
                     <div class="col-lg-4 well">

                       {{ form_widget(module_form) }}
                       {{ form_errors(module_form) }}
                     
                     
                      </br>
                        <button type="submit" class="btn btn-block btn-success">
                                  <i class="fa fa-check"></i>
                        </button>
                  </div>
          </form>
        </div>
{% endblock %}

