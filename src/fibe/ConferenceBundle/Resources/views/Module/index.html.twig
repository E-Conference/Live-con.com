{% extends 'fibeConferenceBundle::base.html.twig' %}

{% block centerPanel %}
  {{ parent() }}
  <h1>Module list</h1>

  <table class="records_list">
    <thead>
      <tr>
        <th>Id</th>
        <th>Papermodule</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      {% for entity in entities %}
        <tr>
          <td><a href="{{ path('conference_module_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
          <td>{{ entity.paperModule }}</td>
          <td>
            <ul>
              <li>
                <a href="{{ path('conference_module_show', { 'id': entity.id }) }}">show</a>
              </li>
              <li>
                <a href="{{ path('conference_module_edit', { 'id': entity.id }) }}">edit</a>
              </li>
            </ul>
          </td>
        </tr>
      {% endfor %}
    </tbody>
  </table>

  <ul>
    <li>
      <a href="{{ path('conference_module_new') }}">
        Create a new entry
      </a>
    </li>
  </ul>
{% endblock %}
