{% extends 'fibeConferenceBundle::base.html.twig' %}

{% block title %}edit conference{{ parent() }}{% endblock title %}

{% block centerPanel %}
  {{ parent() }}


    <form action="{{ path('conference_module_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
        <input type="hidden" name="_method" value="PUT" />
        {{ form_widget(edit_form) }}
        <p>
            <button type="submit">Edit</button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a href="{{ path('conference_module_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('conference_module_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
