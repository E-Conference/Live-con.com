
{% extends 'fibeHomePageBundle::base.html.twig' %}

{% block head %}

    {% block title %}Sympozer upgrade your conference{% endblock %}


    {% block stylesheets_page %}
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/fibehomepage/css/homepage.css') }}" />
        <!-- reset -->
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('bundles/fibehomepage/css/reset.css') }}" />
        <!-- grid system 16 -->
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('bundles/fibehomepage/css/960_12_col.css') }}" />

        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('bundles/fibehomepage/css/style.css') }}" />
    {% endblock %}
{% endblock %}

{% block body %}
    {% include "fibeHomePageBundle::topBar.html.twig" %}

    <div id="login_page">

        {% if error %}
            <div class="alert alert-error">{{ error.message }}</div>
        {% endif %}

        <div id="background"></div>
        <div class="container_12">

            <div id="login" class="grid_4">
                <a href="http:/sympozer.com/" title="sympozer UPGRATE YOUR CONFERENCE"><img src="{{ asset('bundles/fibehomepage/img/logo.png') }}" alt="Livecon" /></a>
                <div style="color:#811B06"><strong>{{ error.message }}</strong></div>
                {% include "FOSUserBundle:Resetting:request_content.html.twig" %}

            </div>
        </div>
    </div>


{% endblock %}


