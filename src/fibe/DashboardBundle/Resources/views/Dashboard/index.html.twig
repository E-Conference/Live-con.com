{% extends 'fibeDashboardBundle::base.html.twig' %}

{% block title %}Dashboard - seamposium{% endblock title %}
{% block body %}


{% include "fibeDashboardBundle::navbar.html.twig" %}
  <div class="container">
     <div class="row center">
              <div class="row-fluid">

                  <div class="col-lg-4 well">
                        <i class="icon-calendar icon-5x" style="display:block;text-align:center"></i>

                          <a href="{{ path('schedule_view') }}" role="button" class="btn btn-info btn-large" style="display:block;text-align:center">
                            <span class="icon-calendar"></span>Manage your schedule
                          </a>
                  </div>
                   <div class="col-lg-4 well">
                       <i class="icon-cog icon-5x"  style="display:block; text-align:center"></i>
                          <a href="{{ path('schedule_conference_show') }}" role="button" class="btn btn-success btn-large" style="display:block;text-align:center">
                            <span class="icon-cog"></span>Manage your conference
                          </a>

                  </div>
                  <div class="col-lg-4 well">
                       <i class="icon-qrcode  icon-5x"  style="display:block; text-align:center"></i>
                          <a href="{{ path('mobileAppTheme_index') }}" role="button" class="btn btn-warning btn-large" style="display:block;text-align:center">
                            <span class="icon-grcode"></span>Manage your mobile applciation
                          </a>

                  </div>
              </div>
      </div>
</div>    

