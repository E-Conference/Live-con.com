        <rdfs:label>Serge Roukine</rdfs:label>
        <foaf:firstName>Serge</foaf:firstName>
        <foaf:lastName>Roukine</foaf:lastName>
        <foaf:mbox_sha1sum></foaf:mbox_sha1sum>
