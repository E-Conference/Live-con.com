	<NamedIndividual rdf:about="http://www.blendconference.com/speakers/mathieu-nebra">
        <rdf:type rdf:resource="&foaf;Person"/>
		<foaf:name>Mathieu Nebra</foaf:name>
        <rdfs:label>Mathieu Nebra</rdfs:label>
		<foaf:firstName>Mathieu</foaf:firstName>
        <foaf:lastName>Nebra</foaf:lastName>
        <foaf:mbox_sha1sum></foaf:mbox_sha1sum>
		<foaf:mbox></foaf:mbox>
