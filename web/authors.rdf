        <rdfs:label>Edouard de la Jonquière</rdfs:label>
		<foaf:firstName>Edouard</foaf:firstName>
        <foaf:lastName>de la Jonquière</foaf:lastName>
        <foaf:mbox_sha1sum></foaf:mbox_sha1sum>
		<foaf:mbox></foaf:mbox>
