sudo php app/console doctrine:database:drop --force
sudo php app/console doctrine:database:create
sudo php app/console doctrine:schema:update --force
sudo php app/console assets:install web
sudo php app/console fos:user:create admin admin@example.com DataConfPower
sudo php app/console fos:user:promote admin ROLE_ADMIN
sudo php app/console wwwconf:database:init
